1 DETAIL = 32
5 START = TIMER
10 SCREEN 9
20 FOR PX = 0 TO 639
21 C$ = INKEY$
22 IF C$ = "q" THEN END
30 FOR PY = 0 TO 349
40 X0 = (PX / 182.571486#)-2.5
50 Y0 = (PY / 174.5) - 1
60 X = 0
70 Y = 0
80 I = 0
90 MI = DETAIL
100 WHILE ((X*X + Y*Y < 4) AND I<MI)
110 XTEMP = X*X - Y*Y + X0
120 Y = 2*X*Y + Y0
130 X = XTEMP
140 I = I + 1
150 WEND
160 PSET (PX,PY),I MOD 16
170 NEXT PY
180 NEXT PX
190 DONE = TIMER
200 LOCATE 1,1 : PRINT "time ";DONE-START
