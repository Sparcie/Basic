5 SCREEN 7
10 DEFINT A-Z
20 START! = TIMER
30 FOR I = 1 TO 1000
40 FOR C = 1 TO 50
50 LINE (C,0)-(C,100),(I+C) MOD 16
60 NEXT C
70 NEXT I
80 DONE! = TIMER
90 LINETIME! = DONE! - START!
100 REM circle test
110 START! = TIMER
120 FOR I= 1 TO 1000
130 FOR C= 1 TO 50
140 CIRCLE (160,100),C,(C+I) MOD 16
150 NEXT C
160 NEXT I
170 DONE! = TIMER
180 CIRCLETIME! = DONE! - START!
200 REM put test
210 DIM A(100)
220 FOR I = 1 TO 10
230 FOR C = 1 TO 10
240 PSET (I,C),INT(RND*16)
250 NEXT C
260 NEXT I
270 GET (1,1)-(10,10),A
280 START!=TIMER
290 FOR I = 1 TO 100
300 FOR C = 0 TO 31
310 FOR Z = 0 TO 19
320 PUT (C*10,Z*10),A,XOR
330 NEXT Z
340 NEXT C
350 NEXT I
360 DONE!=TIMER
370 PUTTIME!=DONE!-START!
380 PRINT "line time ";LINETIME!
390 PRINT "circle time ";CIRCLETIME!
400 PRINT "put time ";PUTTIME!
410 PRINT "lines/second "; 50000!/LINETIME!
420 PRINT "circles/second "; 50000!/CIRCLETIME!
430 PRINT "puts/second "; 64000!/PUTTIME!
